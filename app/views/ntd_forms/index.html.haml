.row
  .col-md-12
    .panel.panel-default.header-part
      .panel-heading
        .row
          .col-md-4
            %h3.panel-title
              %i.fa.fa-list
              NTD Forms
          .col-md-8
            = render 'index_actions'
      .panel-body
        - if @forms.empty?
          %p No forms found
        - else
          .col-md-12
            = render :partial => 'index_table', :locals => {:forms => @forms}
